#!/bin/bash

set -euo pipefail

trap '[ $? -eq 0 ] && exit 0; printf "%s failed\n" "$0"' EXIT
dir="$(CDPATH='' cd -- "$(dirname -- "$0")/.." && pwd -P)"
source "${dir}/bin/utils.sh"

bin="${dir}/bin"

"${bin}/generate-device-description" "azure-device.test.de"

"${bin}/precompute-rtm-manifest-tdx-azure"

"${bin}/precompute-os-manifest-tdx-azure"
